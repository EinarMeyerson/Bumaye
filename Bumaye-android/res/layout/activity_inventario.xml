<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ea.grupo2.Bumaye.android.InventarioActivity" >

    <!-- The main content view -->

    <FrameLayout
        android:id="@+id/content_frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical"
            android:paddingLeft="20dp" >
            
            <LinearLayout
                    android:id="@+id/relativeguantescorazaarma"
                    android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:orientation="horizontal" >
                    
                <ImageView
                        android:id="@+id/objeto1"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:clickable="true"
                        android:onClick="clickImagen"
                        android:paddingRight="20dp"
                        android:src="@drawable/piedra" />
                    
                <ImageView
                        android:id="@+id/objeto2"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:clickable="true"
                        android:onClick="clickImagen"
                        android:paddingRight="20dp"
                        android:src="@drawable/runa_agua" />

                    <ImageView
                        android:id="@+id/objeto3"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:clickable="true"
                        android:onClick="clickImagen"
                        android:paddingRight="20dp"
                        android:src="@drawable/runa_agua2" />

                    <ImageView
                        android:id="@+id/objeto4"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:clickable="true"
                        android:onClick="clickImagen"
                        android:src="@drawable/runa_agua3" />

                    <ImageView
                        android:id="@+id/runa_agua5"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:clickable="true"
                        android:onClick="clickImagen"
                        android:paddingLeft="20dp"
                        android:src="@drawable/runa_agua4" />
                </LinearLayout>
            
            <LinearLayout
                    android:id="@+id/relativeguantescorazaarma"
                    android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:orientation="horizontal" >
                    
                <ImageView
                        android:id="@+id/objeto6"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:clickable="true"
                        android:onClick="clickImagen"
                        android:paddingRight="20dp"
                        android:src="@drawable/runa_cristal_agua" />
                    
                <ImageView
                        android:id="@+id/objeto7"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
