                        android:clickable="true"
                        android:onClick="clickImagen"
                        android:src="@drawable/runa_rayo" />

                    <ImageView
                        android:id="@+id/objeto15"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:clickable="true"
                        android:onClick="clickImagen"
                        android:paddingLeft="20dp"
                        android:src="@drawable/runa_rayo2" />
                </LinearLayout>

            <LinearLayout
                    android:id="@+id/relativeguantescorazaarma"
                    android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:orientation="horizontal" >
                    
                <ImageView
                        android:id="@+id/objeto16"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:clickable="true"
                        android:onClick="clickImagen"
                        android:paddingRight="20dp"
                        android:src="@drawable/max_runa_cristal_agua" />
                    
                <ImageView
                        android:id="@+id/objeto17"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:clickable="true"
                        android:onClick="clickImagen"
                        android:paddingRight="20dp"
                        android:src="@drawable/max_runa_cristal_hoja" />

                    <ImageView
                        android:id="@+id/objeto18"
                        android:layout_width="fill_parent"
